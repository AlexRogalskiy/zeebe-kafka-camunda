[*]
charset=utf-8
end_of_line=lf
insert_final_newline=false
indent_style=space
indent_size=2
trim_trailing_whitespace=true
max_line_length=140

[{*.java,*.kt,*.properties}]
insert_final_newline=true

[*.json]
indent_style=space
indent_size=2

[{*.yml,*.yaml}]
indent_style=space
indent_size=2
insert_final_newline=true

